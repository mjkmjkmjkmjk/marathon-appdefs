{
  "id": "/nginx-docker-host",
  "instances": 2,
  "cpus": 0.01,
  "mem": 32,
  "cmd": "env | sort && sed -i \"s/80/$PORT0/g\" /etc/nginx/conf.d/default.conf && nginx -g \"daemon off;\"",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "nginx:alpine",
      "network": "HOST"
    }
  },
  "portDefinitions": [
    {
      "port": 0,
      "protocol": "tcp",
      "name": "http",
      "labels": {
        "VIP_0": "/nginx-docker-host:80"
      }
    }
  ],
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/",
      "portIndex": 0,
      "gracePeriodSeconds": 20,
      "intervalSeconds": 20,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ],
  "labels": {
    "HAPROXY_0_VHOST": "http://thomaskra-publicsl-r1d7akfn7e94-852276919.us-east-1.elb.amazonaws.com",
    "HAPROXY_GROUP": "external"
  }
}