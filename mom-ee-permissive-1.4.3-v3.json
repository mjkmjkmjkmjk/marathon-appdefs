{
  "id": "/gcp/_DCOS_SPACE_/mom/user-marathon",
  "cmd": "cd $MESOS_SANDBOX && LIBPROCESS_PORT=$PORT1 && /marathon/bin/start --default_accepted_resource_roles \"*\" --enable_features \"vips,task_killing,external_volumes,secrets,gpu_resources\" --framework_name _DCOS_SPACE_-user-marathon --hostname $LIBPROCESS_IP --http_port $PORT0 --master zk://master.mesos:2181/mesos --max_tasks_per_offer 1 --mesos_leader_ui_url /mesos --mesos_role _DCOS_SPACE_-user-marathon-role --zk zk://zk-1.zk:2181,zk-2.zk:2181,zk-3.zk:2181,zk-4.zk:2181,zk-5.zk:2181/mom/_DCOS_SPACE_-user-marathon --mesos_authentication --mesos_authentication_principal dcos__DCOS_SPACE__user_marathon",
  "cpus": 2,
  "mem": 8192,
  "disk": 0,
  "instances": 1,
  "constraints": [
    [
      "hostname",
      "UNIQUE"
    ]
  ],
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "cbb-cs03-bt26-08.svr.us.jpmchase.net:5000/mesosphere/marathon-dcos-ee:v1.4.3_1.9.8",
      "network": "HOST",
      "privileged": false,
      "parameters": [],
      "forcePullImage": false
    }
  },
  "env": {
    "JVM_OPTS": "-Xms256m -Xmx4g",
    "DCOS_STRICT_SECURITY_ENABLED": "false",
    "DCOS_SERVICE_ACCOUNT_CREDENTIAL_TOFILE": {
      "secret": "service-credential"
    },
    "USER": "dcos__DCOS_SPACE__user_marathon",
    "MESOS_AUTHENTICATEE": "com_mesosphere_dcos_ClassicRPCAuthenticatee",
    "MESOS_MODULES": "{\"libraries\":[{\"file\":\"/opt/libmesos-bundle/lib/libdcos_security.so\",\"modules\":[{\"name\":\"com_mesosphere_dcos_ClassicRPCAuthenticatee\"}]}]}",
    "LIBPROCESS_SSL_ENABLED": "false",
    "LIBPROCESS_SSL_SUPPORT_DOWNGRADE": "true",
    "LIBPROCESS_SSL_REQUIRE_CERT": "false",
    "LIBPROCESS_SSL_VERIFY_CERT": "false",
    "PLUGIN_ACS_URL": "http://master.mesos",
    "PLUGIN_AUTHN_MODE": "dcos/jwt+anonymous",
    "PLUGIN_FRAMEWORK_TYPE": "marathon"
},
  "labels" :  {
    "DCOS_SERVICE_NAME":"_DCOS_SPACE_-user-marathon",
    "DCOS_SERVICE_PORT_INDEX": "0",
    "DCOS_SERVICE_SCHEME":"http"
},
    "healthChecks": [
    {
      "path": "/ping",
      "protocol": "HTTP",
      "portIndex": 0,
      "gracePeriodSeconds": 1800,
      "intervalSeconds": 10,
      "timeoutSeconds": 5,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    }
  ],
  "secrets": {
    "service-credential": {
      "source": "/gcp/_DCOS_SPACE_/mom/service-credential"
    }
  },
  "portDefinitions": [
    {
      "port": 0,
      "name": "http",
      "labels": {
        "VIP_0":"/gcp/_DCOS_SPACE_/mom/user-marathon:8080"
      }
    },
    {
      "port": 0,
      "name": "libprocess"
    }
  ]
}
